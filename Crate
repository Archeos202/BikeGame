package ch.epfl.cs107.play.game.actor.crate;

import ch.epfl.cs107.play.game.actor.Actor;
import ch.epfl.cs107.play.game.actor.ActorGame;
import ch.epfl.cs107.play.game.actor.GameEntity;
import ch.epfl.cs107.play.math.Entity;
import ch.epfl.cs107.play.math.Part;
import ch.epfl.cs107.play.math.Vector;

public abstract class Crate extends GameEntity implements Actor {
	private ActorGame game;
	private Entity entity;
	private Part part;

	Crate(ActorGame game, boolean fixed, Vector position, float width, float height, String name) {
		GameEntity(game, fixed, position);
		ImageGraphics(name, width, height);
	}

}
